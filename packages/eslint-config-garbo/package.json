{
  "name": "eslint-config-garbo",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Shared eslint config for the garbo monorepo",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "madge": "madge --circular . ./index.js",
    "build": "echo \"Nothing to do\"",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "peerDependencies": {
    "eslint": "^9.9.0",
    "prettier": "^3.0.3",
    "typescript": ">=4.2.0"
  },
  "dependencies": {
    "@eslint/js": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-libram": "^0.4.31",
    "typescript-eslint": "^8.2.0"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.9.0",
    "lint-staged": "^15.2.9",
    "madge": "^8.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
