{
  "name": "eslint-config-garbo",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Shared eslint config for the garbo monorepo",
  "main": "index.mts",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "madge": "madge --circular . ./index.js",
    "build": "echo \"Nothing to do\"",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "peerDependencies": {
    "eslint": "^9.39.2",
    "prettier": "^3.0.3",
    "typescript": ">=4.2.0"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-libram": "^0.5.1",
    "typescript-eslint": "^8.50.0"
  },
  "devDependencies": {
    "@types/eslint__js": "^9.14.0",
    "eslint": "^9.39.2",
    "lint-staged": "^15.2.9",
    "madge": "^8.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
